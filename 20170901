# 进入Database
crsctl stat res -t
srvctl status database -db <dbname>
export ORACLE_SID=
export ORACLE_HOME=
export PATH=$ORACLE_HOME/bin:$PATH
echo $ORACLE_HOME
sqlplus "/as sysdba"
select open_mode from v$database;
